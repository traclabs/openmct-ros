{
  "name": "openmct-ros",
  "version": "0.0.1-SNAPSHOT",
  "description": "An adapter for connecting Open MCT with ROS",
  "main": "dist/openmct-ros.js",
  "scripts": {
    "clean": "rm -rf ./dist ./node_modules ./package-lock.json",
    "test": "echo \"Error: no test specified\" && exit 1",
