    "eslint": "8.25.0",
    "eventemitter3": "4.0.7",
    "source-map-loader": "^3.0.1",
    "uuid": "9.0.0",
    "webpack": "^5.99.8",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "lru-cache": "11.1.0",
    "roslib": "1.4.1"
